			"Section": "Buildables"}
	},
	"DynamicTokens": [
		{
			"Name": "Version",
			"Value": ""
		},
		{
			"Name": "Version",
			"Value": "expanded/",
			"When": { "HasMod": "VibrantPastoral.Expanded" }
		},
		{
			"Name": "Seasonal",
			"Value": "_{{Season}}"
		},
		{
			"Name": "Seasonal",
			"Value": "",
			"When": { "Season": "Spring" }
		},
		{
			"Name": "SeasonOverride",
			"Value": "{{Season}}"
		},
		{
			"Name": "SeasonOverride",
			"Value": "Spring",
			"When": { "Query: NOT {{IsOutdoors}} OR ( {{LocationName |contains=Desert}} AND {{HasMod |contains=VibrantPastoral.Expanded}} )": true}
		},
		{
			"Name": "SeasonOverride",
			"Value": "Summer",
			"When": { "LocationName": "IslandEast, IslandNorth, IslandSouth, IslandSouthEast, IslandWest" }
		},
		{
			"Name": "FarmLayout",
			"Value": "{{FarmType}}"
		},
		{
			"Name": "FarmLayout",
			"Value": "VibrantPastoral.C/PetBowlShadow",
			"When": { "VibrantPastoral.C/PetBowlShadow": "Beach, Standard" }
		},
		{
			"Name": "FarmLayout",
			"Value": "void",
			"When": {
				"FarmType": "Standard",
				"HasMod": "flashshifter.GrandpasFarm, flashshifter.immersivefarm2remastered"
			}
		},
		{
			"Name": "TreeType",
			"Value": "Vanilla"
		},
		{
			"Name": "TreeType",
			"Value": "Simple Foliage",
			"When": { "HasMod": "Az.Foliage" }
		}
	],
	"Changes": [
	//Inclusions
		//0Main
        {
            "Action": "Include",
            "FromFile": "data/{{Version}}0main.json"
        },
		//Buildings:Farm
        {
            "Action": "Include",
            "FromFile": "data/buildings.json, data/masks.json",
			"When": { "Buildings |contains=farm": true }
        },
		//Build on/Buildable Ginger Island
        {
            "Action": "Include",
            "FromFile": "data/compat/BuildGingerIsland.json",
			"When": { "Query: {{HasMod |contains=Sexynes.CP.BuildOnGingerIslandFarm}} OR {{HasMod |contains=mouahrara.BuildableGingerIslandFarm}}": true }
        },
		//Mail
        {
            "Action": "Include",
            "FromFile": "data/mail.json",
			"When": { "VibrantPastoral.C/EdNygma": true }
        },
		//Patches
        {
            "Action": "Include",
            "FromFile": "data/{{Version}}patches.json"
        },
		//NPCMapLocations
        {
            "Action": "Include",
            "FromFile": "data/compat/NPCMapLocations.json",
			"When": {
				"HasMod": "Bouhm.NPCMapLocations",
				"HasMod |contains=VibrantPastoral.Expanded": false
			}
        },
		//Floors Extra
        {
            "Action": "Include",
            "FromFile": "data/flooring.json",
			"When": { "VibrantPastoral.C/Interiors": true }
        },
		//Furniture
        {
            "Action": "Include",
            "FromFile": "data/{{Version}}furniture.json",
			"When": { "VibrantPastoral.C/Furniture": true }
        },
		//TownInterior
        {
            "Action": "Include",
            "FromFile": "data/interiors.json",
			"When": { "VibrantPastoral.C/Interiors": true }
        }
	]
}